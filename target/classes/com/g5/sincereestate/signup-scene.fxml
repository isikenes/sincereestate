<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #BBE1FA;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.g5.sincereestate.SignUpController">

    <Label alignment="CENTER" layoutX="232.0" layoutY="12.0" prefHeight="77.0" prefWidth="334.0" text="Sincere Estate" textAlignment="CENTER">
        <font>
            <Font name="Consolas Bold" size="36.0" />
        </font>
    </Label>
    <Button layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#GoMainScene" prefHeight="20.0" prefWidth="80.0" style="-fx-background-color: #3282B8; -fx-border-color: #1B262C; -fx-border-width: 2px; -fx-text-fill: #BBE1FA;" text="Back">
        <font>
            <Font name="Consolas" size="20.0" />
        </font>
    </Button>

    <Label layoutX="180.0" layoutY="100.0" text="First Name:">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </Label>
    <TextField fx:id="firstNameTF" layoutX="379.0" layoutY="92.0" prefHeight="44.0" prefWidth="334.0">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </TextField>
    <Label layoutX="180.0" layoutY="175.0" text="Last Name:">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </Label>
    <TextField fx:id="lastNameTF" layoutX="379.0" layoutY="167.0" prefHeight="44.0" prefWidth="334.0">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </TextField>
    <Label layoutX="180.0" layoutY="250.0" text="Email:">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </Label>
    <TextField fx:id="emailTF" layoutX="379.0" layoutY="242.0" prefHeight="44.0" prefWidth="334.0">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </TextField>
    <Label layoutX="180.0" layoutY="325.0" text="Password:">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </Label>
    <PasswordField fx:id="passwordTF" layoutX="379.0" layoutY="317.0" prefHeight="44.0" prefWidth="334.0">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </PasswordField>
    <Label layoutX="180.0" layoutY="400.0" text="Phone Number:">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </Label>
    <TextField fx:id="phoneTF" layoutX="379.0" layoutY="392.0" prefHeight="44.0" prefWidth="334.0">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </TextField>
    <Label layoutX="180.0" layoutY="475.0" text="Birthdate:">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </Label>
    <TextField fx:id="birthdateTF" layoutX="379.0" layoutY="467.0" prefHeight="44.0" prefWidth="334.0" promptText="YYYY-MM-DD">
        <font>
            <Font name="Consolas" size="24.0" />
        </font>
    </TextField>

    <Button layoutX="299.0" layoutY="531.0" mnemonicParsing="false" onAction="#SignUp" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #3282B8; -fx-border-color: #1B262C; -fx-border-width: 3px; -fx-text-fill: #BBE1FA;" text="Sign Up">
        <font>
            <Font name="Consolas" size="26.0" />
        </font>
    </Button>
</AnchorPane>
